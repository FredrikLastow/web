<% title('Utskott') %>
<div class="col-md-offset-10 col-md-2 md-margin-bottom-40">
	<%if can? :edit, @council %> 				      	
	    <%= link_to (content_tag(:i, nil, class: "fa fa-edit") + " Redigera"), edit_council_path(@council), class: "btn-u btn-u-small"%>	    
	<%end%>
</div>
<div class="container">
  <% if @page.present? %>
      <%=render @page%>
  <%end%>
  <% if @council.posts.present? %>
      <%= render 'posts/table', collection: @council.posts %>
  <%end%>
</div>
