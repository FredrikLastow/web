<li class="dropdown">
  <%= link_to t(:administrate), "#", class: "dropdown-toggle",
              data: {toggle: "dropdown", delay: "0", 'close-others' => "false"} %>
  <ul class="dropdown-menu">
    <% if can? :manage, :all  %>
        <li class="dropdown-submenu">
          <a href="">Admin</a>
          <ul class="dropdown-menu">
            <li><%= link_to models_name(User), :users %></li>
            <li><%= link_to models_name(Menu), :menus %></li>
            <li><%= link_to models_name(Constant), :constants %></li>
            <li><%= link_to image_tag('http://samson.fsektionen.se/projects/web/stages/production.svg?token=8d70d6eaf8ef80c828d2f1472e89dc6d'),
                            'http://samson.fsektionen.se/projects/web/stages/production' %></li>
            <li><%= link_to image_tag('http://samson.fsektionen.se/projects/web/stages/dev.svg?token=8d70d6eaf8ef80c828d2f1472e89dc6d'),
                            'http://samson.fsektionen.se/projects/web/stages/dev' %></li>
          </ul>
        </li>
    <% end %>
    <% if can? :manage, Page %>
        <li class="dropdown-submenu">
          <a href=""><%=Page.model_name.human%></a>
          <ul class="dropdown-menu">
            <li><%= link_to "Snabbvy av alla event", :events %></li>
            <li><%= link_to "Nytt event", :new_event %></li>
          </ul>
        </li>
    <%end%>
    <% if can? :modify, Event %>
        <li class="dropdown-submenu">
          <a href=""><%=Event.model_name.human%></a>
          <ul class="dropdown-menu">
            <li><%= link_to "Snabbvy av alla event", :events %></li>
            <li><%= link_to "Nytt event", :new_event %></li>
          </ul>
        </li>
    <% end %>
    <% if can? :modify, News %>
        <li class="dropdown-submenu">
          <a href=""><%=News.model_name.human%></a>
          <ul class="dropdown-menu">
            <li><%= link_to "Ny nyhet", :new_news %></li>
          </ul>
        </li>
    <% end %>
    <% if can? :modify, Council %>
        <li class="dropdown-submenu">
          <a href=""><%=Council.model_name.human%></a>
          <ul class="dropdown-menu">
            <% for council in Council.all %>
                <% if can? :manage, council %>
                    <li><%= link_to council.title, edit_council_path(council) %></li>
                <% end %>
            <% end %>
          </ul>
        </li>
    <% end %>
    <% if can? :manage, CafeWork %>
        <li class="dropdown-submenu">
        <%= link_to(CafeWork.model_name.human, :admin_hilbert)%>
        </li>
    <% end %>
  </ul>
</li>
